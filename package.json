{
  "name": "diffsense",
  "displayName": "DiffSense",
  "description": "智能Git代码影响分析工具，支持Java、Golang、前端代码的变更影响分析和可视化",
  "version": "0.1.16",
  "publisher": "humphreyLi",
  "author": {
    "name": "DiffSense Team"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/GoldenSupremeSaltedFish/DiffSense-Plugin.git"
  },
  "bugs": {
    "url": "https://github.com/GoldenSupremeSaltedFish/DiffSense-Plugin/issues"
  },
  "homepage": "https://github.com/GoldenSupremeSaltedFish/DiffSense-Plugin#readme",
  "keywords": [
    "git",
    "code analysis",
    "impact analysis",
    "java",
    "golang",
    "frontend",
    "diff",
    "code review"
  ],
  "engines": {
    "vscode": "^1.74.0",
    "node": ">=18.0.0"
  },
  "categories": [
    "Other",
    "Testing",
    "Visualization"
  ],
  "activationEvents": [],
  "main": "dist/extension.js",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "contributes": {
    "commands": [
      {
        "command": "diffsense.runAnalysis",
        "title": "Run Analysis",
        "category": "DiffSense"
      },
      {
        "command": "diffsense.refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "diffsense.showOutput",
        "title": "显示输出日志",
        "category": "DiffSense"
      }
    ],
    "views": {
      "diffsense": [
        {
          "type": "webview",
          "id": "diffsense.analysisView",
          "name": "DiffSense",
          "when": "true"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "diffsense",
          "title": "DiffSense",
          "icon": "$(git-branch)"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "diffsense.refresh",
          "when": "view == diffsense.analysisView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "compile": "tsc",
    "watch": "tsc -w",
    "prepare-package": "node scripts/prepare-package.js",
    "clean-package": "node scripts/clean-package.js",
    "package": "npm run prepare-package && node scripts/package-vsix.js",
    "publish": "npm run prepare-package && vsce publish",
    "dev": "npm run watch",
    "build": "npm run clean && tsc && npm run prepare-package",
    "build-ui": "cd ui/diffsense-frontend && npm run build",
    "dev-ui": "cd ui/diffsense-frontend && npm run dev",
    "clean": "rimraf dist out *.vsix",
    "test": "npm run compile && node ./dist/test/runTest.js",
    "prepare-release": "node scripts/prepare-release.js"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "glob": "^10.3.0"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "18.x",
    "@types/vscode": "^1.74.0",
    "@vscode/vsce": "^2.24.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.2.2"
  }
}
